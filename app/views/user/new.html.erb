<% form_for , url: {action: "create"} do |f| %>

	<div>
	  <%= f.label :email %><br>
	  <%= f.email_field :email %>
	</div>
	<div>
	  <%= f.label :password %><br>
	  <%= f.password_field :password %>
	</div>
	<div>
	  <%= f.label :password_confirmation %><br>
	  <%= f.password_field :password_confirmation %>
	<div>
	  <%= f.submit %>
	</div>
	</div>

<% end %>

