<h1>User#edit</h1>
<p>Find me in app/views/user/edit.html.erb</p>
<div class='row'>
<% @comedians.each do |comedian| %>
<div class='col s4'>
	<div class='card'>
		<div class='card-image'>
			<img src=<%= comedian.image %>>
			<span class='card-title'> <%= comedian.name %> </span>
		</div>
		<div class="card-action">
      <%= link_to edit_user_path(@user), remote: true do %>
    <button>Edit</button>
  <% end %>
    </div>
	</div>
</div>
<% end %>
</div>