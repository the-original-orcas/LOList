<h1>Comedian#index</h1>
<p>hello</p>
<p>Find me in app/views/comedian/index.html.erb</p>
<%= link_to edit_user_path  %>
<div id="fb-root"></div>
<button name="my_full_name" onclick="ShowMyName()" value="My Name"></button>
<script language="javascript" type="text/javascript">
function ShowMyName() {
        FB.api("/me",
                function (response) {
                    alert('Name is ' + response.name);
                });
         
    }
</script>


<div class='row'>
<% @comedians.each do |comedian| %>
<div class='col s4'>
	<div class='card'>
		<div class='card-image'>
			<img src=<%= comedian.image %>>
			<span class='card-title'> <%= comedian.name %> </span>
		</div>
		<div class="card-action">
      <a id='comedian-<%= comedian.id %>' href="#">Follow</a>
    </div>
	</div>
</div>
<div
  class="fb-like"
  data-share="true"
  data-width="250"
  data-show-faces="true">
</div>
<% end %>
</div>




